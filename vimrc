filetype on
filetype plugin indent on

set nocompatible
set modelines=0

set t_Co=256
colors wombat256
syntax on

set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab

set ai
set encoding=utf-8
set scrolloff=3
set showmode
set showcmd
set hidden
set wildmenu
set wildmode=list:longest
set visualbell
set cursorline
set ttyfast
set ruler
set backspace=indent,eol,start
set laststatus=2
set undofile
set number
set wrap
set textwidth=79
set formatoptions=qrn1

